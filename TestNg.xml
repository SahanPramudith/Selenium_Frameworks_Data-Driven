<?xml version="1.0" encoding="utf-8" ?>

<suite name="LoginTest">
    <test name="BothCorrect">
        <parameter name="username" value="Admin"/>
        <parameter name="password" value="admin123"/>
        <parameter name="validation" value="valid"/>
        <classes>
            <class name="LoginTestCasses.Bothcorrect"/>
        </classes>
    </test>
    <test name="BothIncorrect">
        <parameter name="username" value="admin123"/>
        <parameter name="password" value="admin"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="LoginTestCasses.BothIncorrect"/>
        </classes>
    </test>
    <test name="correctPassword">
        <parameter name="username" value="Admin"/>
        <parameter name="password" value="admin123"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="LoginTestCasses.CorrectPassword"/>
        </classes>
    </test>
    <test name="IncorrectPassword">
        <parameter name="username" value="sahana"/>
        <parameter name="password" value="sahan123"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="LoginTestCasses.InCorrectPassword"/>
        </classes>
    </test>
</suite>
