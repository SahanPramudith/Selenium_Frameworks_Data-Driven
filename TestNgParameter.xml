<?xml version="1.0" encoding="utf-8" ?>


<suite name="LoginTest">
    <test name="BothCorrect">
        <parameter name="username" value="Admin"/>
        <parameter name="password" value="admin123"/>
        <parameter name="validation" value="valid"/>
        <classes>
            <class name="DataDrivenTest.LoginUsingParameters"/>
        </classes>
    </test>
    <test name="BothIncorrect">
        <parameter name="username" value="admin123"/>
        <parameter name="password" value="admin"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="DataDrivenTest.LoginUsingParameters"/>
        </classes>
    </test>
    <test name="correctPassword">
        <parameter name="username" value="Admi"/>
        <parameter name="password" value="admin123"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="DataDrivenTest.LoginUsingParameters"/>
        </classes>
    </test>
    <test name="IncorrectPassword">
        <parameter name="username" value="Admin"/>
        <parameter name="password" value="admin123"/>
        <parameter name="validation" value="Invalid"/>
        <classes>
            <class name="DataDrivenTest.LoginUsingParameters"/>
        </classes>
    </test>
</suite>
